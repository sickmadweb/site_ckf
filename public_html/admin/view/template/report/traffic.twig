{{ header }}{{ column_left }}
<div id="content">
  <div class="page-header">
    <div class="container-fluid">
      <h1>{{ heading_title }}</h1>
      <ul class="breadcrumb">
        {% for breadcrumb in breadcrumbs %}
        <li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
        {% endfor %}
      </ul>
    </div>
  </div>
  <div class="container-fluid">{% if error_warning %}
    <div class="alert alert-danger alert-dismissible"><i class="fa fa-exclamation-circle"></i> {{ error_warning }}
      <button type="button" class="close" data-dismiss="alert">&times;</button>
    </div>
    {% endif %}
    {% if success %}
    <div class="alert alert-success alert-dismissible"><i class="fa fa-check-circle"></i> {{ success }}
      <button type="button" class="close" data-dismiss="alert">&times;</button>
    </div>
    {% endif %}
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title"><i class="fa fa-list"></i> {{ text_list }}</h3>
      </div>
      <div class="panel-body">
        <div class="table-responsive">
          <table class="table table-bordered">
            <thead>
              <tr>
                <td class="text-left">{{ column_site }}</td>
                <td class="text-right">{{ column_users }}</td>
                <td class="text-right">{{ column_repeated }}</td>
                <td class="text-right">{{ column_phone }}</td>
                <td class="text-right">{{ column_contacts }}</td>
                <td class="text-right">{{ column_viber }}</td>
                <td class="text-right">{{ column_whatsapp }}</td>
                <td class="text-right">{{ column_checkout }}</td>
                <td class="text-right">{{ column_order }}</td>
                <td class="text-right">{{ column_email }}</td>
                <td class="text-right">{{ column_configurator }}</td>
                <td class="text-right">{{ column_time_less_1}}</td>
                <td class="text-right">{{ column_time_1m_5m }}</td>
                <td class="text-right">{{ column_time_more_5m }}</td>
              </tr>
            </thead>
            <tbody>
              {% if reports %}
              {% for report in reports %}
              <tr>
                <td class="text-left">{{ report.site }}</td>
                <td class="text-left">{{ report.users }}</td>
                <td class="text-left">{{ report.repeated }}</td>
                <td class="text-left">{{ report.telephone }}</td>
                <td class="text-left">{{ report.contacts }}</td>
                <td class="text-left">{{ report.viber }}</td>
                <td class="text-left">{{ report.whatsapp }}</td>
                <td class="text-left">{{ report.checkout }}</td>
                <td class="text-left">{{ report.orders }}</td>
                <td class="text-left">{{ report.email }}</td>
                <td class="text-left">{{ report.configurator }}</td>
                <td class="text-left">{{ report.time_less_1 }}</td>
                <td class="text-left">{{ report.time_1m_5m  }}</td>
                <td class="text-left">{{ report.time_more_5m }}</td>
              </tr>
              {% endfor %}
              {% else %}
              <tr>
                <td class="text-center" colspan="3">{{ text_no_results }}</td>
              </tr>
              {% endif %}
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>
{{ footer }}